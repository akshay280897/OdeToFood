@model IEnumerable<OdeToFood.Models.RestaurantListView>
<div Id="restaurantList">


    @foreach (var item in Model)
    {
        <div>
            <h4>@item.Name</h4>
            <h4>@item.City , @item.Country</h4>
        </div>
        <div>Reviews: @item.CountofReviews</div>
        <hr />
    }
</div>

